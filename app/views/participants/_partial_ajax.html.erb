<table>
	<thead>
		<tr>
			<th>name</th>
			<th>classification</th>
			<th>status</th>
			<th colspan="3"></th>
			<th colspan="3"></th>
		</tr>
	</thead>
	<tbody>
		<% @participants.each do |participant| %>
			<tr>
				<td><%= participant.name %></td>
				<td><%= participant.classification_id %></td>
				<td><%= participant.status %></td>
				<td><%= link_to 'detail', participant %></td>
				<% unless participant.status%>
					<td><%= button_to 'check', check_participant_path(participant), method: :put %></td>
				<% end %>
			</tr>
		<% end %>
	</tbody>
</table>
